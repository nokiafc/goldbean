<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <include layout="@layout/layout_base_title"/>
    <ImageView
        android:layout_height="46dp"
        android:layout_width="46dp"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/icon_cash_sucess"
        android:layout_marginTop="31dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:textColor="@color/text_color_5"
        android:layout_marginTop="12dp"
        android:textSize="15dp"
        android:text="提现成功"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:textColor="@color/text_color_9"
        android:layout_marginTop="12dp"
        android:textSize="12dp"
        android:text="小主再接再厉，捞的金豆越多现金越多哦"/>

    <Button
        android:id="@+id/btn_get_bean"
        android:layout_width="282dp"
        android:layout_height="45dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="86dp"
        android:text="继续捞金豆"
        android:textColor="@color/text_color_3"
        android:textSize="16dp"
        android:background="@drawable/shape_button"/>





</LinearLayout>
